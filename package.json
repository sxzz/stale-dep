{
  "name": "stale-dep",
  "version": "0.7.0",
  "packageManager": "pnpm@9.15.5",
  "description": "Check your node_modules matches your package.json.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/stale-dep#readme",
  "bugs": {
    "url": "https://github.com/sxzz/stale-dep/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/stale-dep.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli.js",
    "./nuxt": "./dist/nuxt.js",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "bin": {
    "stale-dep": "./bin/stale-dep.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "pnpm check && tsdown",
    "test": "tsx ./src/cli.ts --update && vitest",
    "check": "tsx ./src/cli.ts",
    "update-hash": "tsx ./src/cli.ts --update",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/schema": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.4.0",
    "find-up": "^7.0.0",
    "fs-extra": "^11.3.0",
    "package-manager-detector": "^0.2.9"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.15.4",
    "@nuxt/schema": "^3.15.4",
    "@sxzz/eslint-config": "^5.0.0",
    "@sxzz/prettier-config": "^2.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.0",
    "bumpp": "^10.0.1",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "tsdown": "^0.5.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
