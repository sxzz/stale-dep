{
  "name": "stale-dep",
  "version": "0.7.0",
  "packageManager": "pnpm@8.10.5",
  "description": "Check your node_modules matches your package.json.",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/stale-dep#readme",
  "bugs": {
    "url": "https://github.com/sxzz/stale-dep/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/stale-dep.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.ts",
        "import": "./dist/index.d.mts"
      },
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./cli": {
      "types": {
        "require": "./dist/cli.d.ts",
        "import": "./dist/cli.d.mts"
      },
      "require": "./dist/cli.js",
      "import": "./dist/cli.mjs"
    },
    "./nuxt": {
      "types": {
        "require": "./dist/nuxt.d.ts",
        "import": "./dist/nuxt.d.mts"
      },
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "bin": {
    "stale-dep": "./bin/stale-dep.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "pnpm check && tsup",
    "test": "tsx ./src/cli.ts --update && vitest",
    "check": "tsx ./src/cli.ts",
    "update-hash": "tsx ./src/cli.ts --update",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/schema": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "find-up": "^7.0.0",
    "fs-extra": "^11.1.1",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.8.1",
    "@nuxt/schema": "^3.8.1",
    "@sxzz/eslint-config": "^3.7.4",
    "@sxzz/prettier-config": "^2.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.9.2",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
