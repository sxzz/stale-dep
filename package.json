{
  "name": "stale-dep",
  "version": "0.8.4",
  "packageManager": "pnpm@10.20.0",
  "description": "Check your node_modules matches your package.json.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/stale-dep#readme",
  "bugs": {
    "url": "https://github.com/sxzz/stale-dep/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/stale-dep.git"
  },
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli.js",
    "./nuxt": "./dist/nuxt.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "bin": {
    "stale-dep": "./bin/stale-dep.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "tsc --noEmit",
    "build": "pnpm check && tsdown",
    "test": "tsx ./cli.ts --update && vitest",
    "check": "tsx ./cli.ts",
    "update-hash": "tsx ./cli.ts --update",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/schema": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.4.2",
    "package-manager-detector": "^1.5.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^4.2.0",
    "@nuxt/schema": "^4.2.0",
    "@sxzz/eslint-config": "^7.2.8",
    "@sxzz/prettier-config": "^2.2.4",
    "@types/node": "^24.9.2",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.12",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "prettier": "@sxzz/prettier-config"
}
