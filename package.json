{
  "name": "stale-dep",
  "version": "0.7.0",
  "packageManager": "pnpm@9.0.6",
  "description": "Check your node_modules matches your package.json.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/stale-dep#readme",
  "bugs": {
    "url": "https://github.com/sxzz/stale-dep/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/stale-dep.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./cli": {
      "require": "./dist/cli.cjs",
      "import": "./dist/cli.js"
    },
    "./nuxt": {
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "bin": {
    "stale-dep": "./bin/stale-dep.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "pnpm check && tsup",
    "test": "tsx ./src/cli.ts --update && vitest",
    "check": "tsx ./src/cli.ts",
    "update-hash": "tsx ./src/cli.ts --update",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/schema": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.11.2",
    "@nuxt/schema": "^3.11.2",
    "@sxzz/eslint-config": "^3.9.1",
    "@sxzz/prettier-config": "^2.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "eslint": "^9.1.1",
    "fast-glob": "^3.3.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.1"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "prettier": "@sxzz/prettier-config"
}
