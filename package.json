{
  "name": "stale-dep",
  "version": "0.8.1",
  "packageManager": "pnpm@10.6.2",
  "description": "Check your node_modules matches your package.json.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/stale-dep#readme",
  "bugs": {
    "url": "https://github.com/sxzz/stale-dep/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/stale-dep.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli.js",
    "./nuxt": "./dist/nuxt.js",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "bin": {
    "stale-dep": "./bin/stale-dep.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "pnpm check && tsdown",
    "test": "tsx ./cli.ts --update && vitest",
    "check": "tsx ./cli.ts",
    "update-hash": "tsx ./cli.ts --update",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/schema": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.4.0",
    "package-manager-detector": "^1.0.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.16.0",
    "@nuxt/schema": "^3.16.0",
    "@sxzz/eslint-config": "^6.0.1",
    "@sxzz/prettier-config": "^2.2.1",
    "@types/node": "^22.13.10",
    "bumpp": "^10.0.3",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "tsdown": "^0.6.5",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
